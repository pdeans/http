    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Stearns",
            "email": "pdeans1986@gmail.com",
            "homepage": "http://patrickstearns.us"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "laminas/laminas-diactoros": "^3.0"
    },
    "autoload": {
        "psr-4": {
